	<queryString>
		<![CDATA[select
sod.sale_order_id
,so.inv_no
,so.inv_date

, 'Sharobi Technologies Pvt. Ltd.' seller_name
, 'YewMed Pharmacy POS Solution' seller_industry
, 'CA-231,Salt Lake,Sector 1, Kolkata' seller_address
, '+91- 3340084321' seller_phone
, 'yewmed@sharobi.com' seller_email
, 'WBSIPL2019002' seller_license
, '19AAACI1681G1ZM' seller_gstn

, concat(ifnull(u.fname,''),' ', ifnull(u.lname,'')) cust_name
, concat( ifnull(ua.street_address,''),', ',ifnull(cit.name,'') ,', ' , ifnull(st.name,'') , ', ' , ifnull(ua.pincode,'') ,', ' , ifnull(ct.name,'') ) cust_address
,ifnull(ua.contact_phone,'') cust_phone
-- ,ifnull(ua.invoice_mail,'') cust_email
,ifnull(u.email,'') cust_email
,i.name item_name
,i.HSN_code
,sod.sale_rate
,sod.pack_qty
,ifnull(ut.code,'') UoM
,sod.gross_amount
,so.gross_amount tot_gross_amt
, sod.disc_per
,sod.disc disc_amount
,so.disc_amount tot_disc_amount
,sod.tax_percentage
,sod.tax_amount
,so.tax_amount tot_tax_amount
,sod.net_amount
,so.net_amount tot_net_amount
,shipping_charge
,tot_shipping_charge

-- ,AmountToWords(sod.net_amount)

from sal_t_sale_order_details sod
inner join sal_t_sale_order so on so.id = sod.sale_order_id
inner join gen_m_user u on u.id = so.customer_id
inner join gen_m_user_address ua on ua.id = so.customer_address_id
left outer join com_m_countries ct on ct.id = ua.country_id
left outer join com_m_states st on st.id = ua.state_id
left outer join com_m_cities cit on cit.id = ua.city_id

inner join in_m_item i on i.id = sod.item_id
left outer join in_m_unit ut on ut.id = i.pack_unit_id

where sod.sale_order_id = $P{W_SaleOrderID} ;]]>
	</queryString>
